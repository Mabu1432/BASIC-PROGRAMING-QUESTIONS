<script>

function count(s, c)
    {
        let res = 0;
 
        for (let i = 0; i < s.length; i++)
        {
            if (s.charAt(i) == c)
            res++;
        }
        return res;
    }
     
        let str= "geeksforgeeks";
        let c = 'e';
        document.write(count(str, c));
         
 </script
